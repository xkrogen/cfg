{
  "mcpServers": {
    "captain": {
      "type": "stdio",
      "command": "captain",
      "args": [
        "start"
      ],
      "env": {
        "NAMESPACES": "data.*,jira,google,github"
      }
    },
    "playwright-extension": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--extension"
      ]
    }
  }
}
